<Window
	x:Class="VidCoder.View.DebugJsonDialog"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:VidCoder.View"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Title="Debug Encode JSON"
	Width="580"
	Height="526"
	mc:Ignorable="d">
	<Grid>
		<TextBox Name="textBox" Margin="0 0 0 40" TextWrapping="Wrap" AcceptsReturn="True" />
		<Button VerticalAlignment="Bottom" HorizontalAlignment="Right" Content="OK" MinWidth="90" MinHeight="30" Margin="5" Click="OnOKClick" />
	</Grid>
</Window>
